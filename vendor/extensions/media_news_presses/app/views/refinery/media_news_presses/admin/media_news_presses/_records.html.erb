<%= render 'refinery/admin/search_header', :url => refinery.media_news_presses_admin_media_news_presses_path %>
<div class='pagination_container'>
  <% if @media_news_presses.any? %>
    <%= render 'media_news_presses' %>
  <% else %>
    <p>
      <% if searching? %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% else %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% end %>
    </p>
  <% end %>
</div>
